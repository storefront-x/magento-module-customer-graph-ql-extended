    "type": "magento2-module",
    "license": [
        "MIT"
    ],
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.4",
        "magento/framework": "*",
        "magento/module-graph-ql": "*",
        "magento/module-customer-graph-ql": "*",
        "magento/module-store": "*"
    },
    "autoload": {
        "files": [
            "registration.php"
        ],
        "psr-4": {
            "StorefrontX\\CustomerGraphQlExtended\\": ""
        }
    },
    "abandoned": true

}

